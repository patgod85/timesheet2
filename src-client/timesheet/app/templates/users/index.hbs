{{bread-crumbs}}


{{#filtered-list items=model.users filter=model.teams headers=model.headers id="teamId" as |user id|}}

    <tr>
        <td>{{sum id 1}}</td>
        <td>{{#link-to "users.user" user.id}}{{user.username}}{{/link-to}}</td>
        <td>{{user.name}} {{user.surname}}</td>
        <td>
            {{#each user.roles as |role|}}
                {{role}}
            {{/each}}
        </td>
    </tr>
{{/filtered-list}}

<p class="pull-right">
    {{link-to "New user" "users.new" class="btn btn-primary"}}
</p>

{{outlet}}
